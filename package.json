{
  "name": "snap.hutao.docs",
  "version": "2.0.0",
  "description": "Documents site for Snap Hutao project",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "docs:build": "vuepress build docs",
    "docs:clean-dev": "vuepress dev docs --clean-cache",
    "docs:dev": "vuepress dev docs",
    "lint": "prettier --check --write .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-beta.66",
    "@vuepress/client": "2.0.0-beta.66",
    "@vuepress/plugin-docsearch": "2.0.0-beta.66",
    "@vuepress/plugin-google-analytics": "2.0.0-beta.66",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "prettier": "2.8.8",
    "vue": "^3.3.4",
    "vuepress": "2.0.0-beta.66",
    "vuepress-plugin-redirect": "2.0.0-beta.235",
    "vuepress-theme-hope": "2.0.0-beta.235"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "nano-staged": {
    "*.md": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@8.3.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom"
      ]
    }
  }
}
